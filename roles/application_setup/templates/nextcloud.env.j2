# GENERAL
PHP_MEMORY_LIMIT=2048M

# The domain from where you'll access Nextcloud (eg. cloud.myself.com)
MY_DOMAIN=cloud.{{ web_domain}}
# Your timezone
TZ=Europe/Paris

# NEXTCLOUD-MARIADB

# The database's root user password
# ! Set a stronger password !
MYSQL_ROOT_PASSWORD={{ mariadb_root_password }}
# The database's nextcloud user password
# ! Set a stronger password !
MYSQL_PASSWORD={{ mariadb_nextcloud_password }}
MYSQL_DATABASE=nextcloud
MYSQL_USER=nextcloud

# NEXTCLOUD
MYSQL_HOST=database
REDIS_HOST=redis

#IMT calendar
CRABLENDAR_USERNAME={{ imt_username }}
CRABLENDAR_PASSWORD={{ imt_password }}
CRABLENDAR_URL={{ imt_url }}


ONLYOFFICE_JWT_SECRET={{ onlyoffice_jwt_secret }}